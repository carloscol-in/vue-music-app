{
    "root": true,
    "env": {
        "node": true,
        "es6": true,
        "mocha": true
    },
    "extends": [
        "plugin:vue/recommended"
    ],
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2018
    },
    "rules": {
        "no-new": 0
    },
    "plugins": [
        "html",
        "vue"
    ]
}